<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstaid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Nosebleed"></activity>
        <activity android:name=".Hypothermia" />
        <activity android:name=".Heart_Stroke" />
        <activity android:name=".Heart_Attack" />
        <activity android:name=".Diabetic_Emergency" />
        <activity android:name=".Allergies" />
        <activity android:name=".Burns" />
        <activity android:name=".Choking" />
        <activity android:name=".Bone" />
        <activity android:name=".Bleeding" />
        <activity android:name=".Asthma" />
        <activity android:name=".Head_Injury" />
        <activity android:name=".Covid19" />
        <activity android:name=".Home" />
        <activity android:name=".SignUp" />
        <activity android:name=".LoginScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>